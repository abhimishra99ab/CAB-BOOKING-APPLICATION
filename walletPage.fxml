      <TextField fx:id="add_amt" layoutX="107.0" layoutY="185.0" promptText="Rs. " />
      <Button fx:id="show_available_bal" layoutX="110.0" layoutY="57.0" mnemonicParsing="false" onAction="#showAvailableBalance" prefHeight="31.0" prefWidth="181.0" text="Show Available Balance" />
